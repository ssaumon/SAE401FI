openapi: 3.0.3
info:
  title: rapport
  description: |-
    Gestion des clients et des livres disponibles dans la librairie

  version: 0.1.0

tags:
  - name: rapports
    description: Consultation  SBOMs

paths:
  /pdf/{idprojet}:
    get:
      tags:
        - SBOMs
      summary: creer un pdf du rapport d'un projet
      parameters:
        - name: idprojet
          in: path
          required: true
          schema:
            type: integer
            format: int64
      responses:
        '200':
          description: successful operation
          content:
            application/pdf:
              schema:
                type: string
                format: binary
        '400':
          description: sbom non importé
          content:
            text/plain:
              schema:
                type: string
        '401':
          description: projet non importé
          content:
            text/plain:
              schema:
                type: string


components:
  schemas:
    prjet:
      type: object
      properties:
        id:
          type: integer
          format: int64
          example: 10
        contenu:
          type: string
          example: contenu du fichier sbom
        idUser:
          type: integer
          format: int64
          example: 10
        date:
          type: string
          format: date
          example: 2025-03-21
